CREATE TABLE tb_usuario (
   id_usuario INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
   nome VARCHAR(100) NOT NULL,
   cpf VARCHAR(100) NOT NULL,
   matricula VARCHAR(100) NOT NULL,
   profissao VARCHAR(100) NOT NULL,
   usuario VARCHAR(100) NOT NULL,
   senha VARCHAR(100) NOT NULL
);

CREATE TABLE tb_disciplina (
   id_disciplina INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
   nome_disciplina VARCHAR(100) NOT NULL,
   tipo VARCHAR(100) NOT NULL,
   professor_id INT NOT NULL, 
   CONSTRAINT `fk_professor_disciplina` FOREIGN KEY (professor_id) REFERENCES tb_usuario (id_usuario)
);

CREATE TABLE tb_aluno_disciplina (
   aluno_id INT NOT NULL,
   disciplina_id INT NOT NULL, 
   nota INT NOT NULL,
   CONSTRAINT `fk_aluno` FOREIGN KEY (aluno_id) REFERENCES tb_usuario (id_usuario),
   CONSTRAINT `fk_disciplina` FOREIGN KEY (disciplina_id) REFERENCES tb_disciplina (id_disciplina)
);

CREATE TABLE tb_dependencia (
   id INT NOT NULL,
   disciplina_id INT NOT NULL,
   CONSTRAINT `fk_dependencia_segunda` FOREIGN KEY (disciplina_id) REFERENCES tb_disciplina (id_disciplina)
);


-- 
create table tb_tarefa(
   id int AUTO_INCREMENT primary key,
   descricao varchar(255) NOT NULL,
   colaborador1 int,
   colaborador2 int,
   colaborador3 int,
   CONSTRAINT `fk_colabordor1` FOREIGN KEY (colaborador1) REFERENCES tb_usuario (id),
    CONSTRAINT `fk_colabordor2` FOREIGN KEY (colaborador2) REFERENCES tb_usuario (id),
     CONSTRAINT `fk_colabordor3` FOREIGN KEY (colaborador3) REFERENCES tb_usuario (id)

   );
   alter table tb_tarefa add column notificacao DATETIME;

